{
  "name": "esm-demo-webpack-build",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm run dev",
    "build": "pnpm run clean && pnpm run compile",
    "test": "echo 'No tests for build package'",
    "clean": "rm -rf ../../results/webpack-esm/*",
    "dev": "webpack --config webpack.config.js --mode=development --watch",
    "compile": "webpack --config webpack.config.js --mode=production",
    
    "webpack": "pnpm run compile",
    "watch": "pnpm run dev"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/react": "^18.2.45",
    "babel-loader": "^9.1.3",
    "react": "^18.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
