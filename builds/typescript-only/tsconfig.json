{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "../../results/typescript-raw",
    "rootDir": "../../src",
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "include": [
    "../../src/**/*"
  ],
  "exclude": [
    "../../src/**/*.test.ts",
    "../../src/**/*.test.tsx"
  ]
}